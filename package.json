{
  "name": "my-picture-memory",
  "productName": "MyPictureMemory",
  "version": "1.1.0",
  "description": "simple picture memory game",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/app.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . my-picture-memory --overwrite --platform=win32 --arch=ia32 --asar --icon=assets/icons/win/app.ico --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --prune=true --version-string.ProductName=\"MyPictureMemory\"",
    "package-linux": "electron-packager . my-picture-memor --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/app.png --prune=true --out=release-builds"
  },
  "author": "Stefan Eder",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/MasterEda92/my-picture-memory"
  },
  "devDependencies": {
    "electron-packager": "^13.1.1",
    "electron": "^5.0.0",
    "bootstrap": "^4.3.1",
    "jquery": "^3.5.0",
    "popper.js": "^1.15.0"
  },
  "dependencies": {
    "bootstrap": "^4.3.1",
    "jquery": "^3.5.0",
    "popper.js": "^1.15.0"
  }
}
